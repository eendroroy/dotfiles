theme = dark:e_ink_dark,light:e_ink_bright

font-family = ""
font-family = "JetBrainsMono Nerd Font Mono"
font-size = 18
font-style-bold = "SemiBold"
font-style-bold-italic = "SemiBold Italic"

macos-icon = paper

shell-integration = zsh
selection-clear-on-typing = true
