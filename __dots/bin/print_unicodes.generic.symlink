#!/usr/bin/env bash

export LANG=en_US.UTF-8

#for ((i=0; i<=65535; i++)); do
for ((i=$((0x0000)); i<=$((0xffff)); i++)); do
  escape_sequence=$(printf "\\\u%04x" "$i")
#  printf "%b          U+%04X  " "$escape_sequence" "$i"
  printf "%b    " "$escape_sequence"
  if (( (i + 1) % 24 == 0 )); then
    echo
  fi
done
