#!/usr/bin/env bash

echo "Installing dotfiles"

echo "Initializing submodule(s)"
git submodule update --init --recursive
git submodule foreach pull origin master

bash link.sh
bash `uname`/recipes/antigen.sh
bash `uname`/recipes/hg-color.sh
bash `uname`/recipes/vim-plug.sh
bash `uname`/recipes/sdkman.sh
